{
  "name": "strawberry",
  "displayName": "Strawberry",
  "description": "Strawberry Syntax Highlighting Support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "strawberry",
      "aliases": ["Strawberry", "strawberry"],
      "extensions": [".berry"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
